    "android": {
      "edgeToEdgeEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#2C1E5C"
      },
      "package": "com.davidmaranho.DicionarioDosNomes",
      "permissions": [
        "INTERNET",
        "com.google.android.gms.permission.AD_ID"
      ],
      "versionCode": 11
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/adaptive-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/adaptive-icon-bg.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#2C1E5C"
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-5781907132925477~5189619634",
          "iosAppId": "ca-app-pub-5781907132925477~5189619634"
        }
      ],
      [
        "react-native-edge-to-edge",
        {
          "android": {
            "parentTheme": "Default",
            "enforceNavigationBarContrast": false
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
