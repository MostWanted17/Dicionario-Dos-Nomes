{
  "expo": {
    "name": "Dicionario Dos Nomes",
    "slug": "Dicionario-Dos-Nomes",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/images/adaptive-icon-bg.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon-bg.png",
        "backgroundColor": "#6c6c6b"
      },
      "package": "com.davidmaranho.DicionarioDosNomes",
      "permissions": ["com.google.android.gms.permission.AD_ID"],
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
         {
          "image": "./assets/images/adaptive-icon-bg.png",
          "resizeMode": "cover",
          "backgroundColor": "#6c6c6b"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
