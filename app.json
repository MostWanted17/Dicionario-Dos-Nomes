{
  "expo": {
    "name": "Dicionario Dos Nomes",
    "slug": "dicionario-dos-nomes",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/adaptive-icon-bg.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon-bg.png",
        "backgroundColor": "#6c6c6b"
      },
      "package": "com.davidmaranho.DicionarioDosNomes",
      "permissions": [
        "com.google.android.gms.permission.AD_ID"
      ],
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/adaptive-icon-bg.png",
          "resizeMode": "cover",
          "backgroundColor": "#6c6c6b"
        }
      ],
      [
      "react-native-google-mobile-ads",
      {
        "android_app_id": "ca-app-pub-5781907132925477~5189619634"
      }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1ec27487-9163-4176-9196-40539bfd92d1"
      }
    }
  }
}
